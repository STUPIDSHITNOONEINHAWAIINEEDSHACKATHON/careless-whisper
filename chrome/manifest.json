{
  "manifest_version": 2,
  "name": "Careless Whisper Messsenging",
  "short_name": "carelesswhis",
  "version": "0.0.2",

  "background": {
    "scripts": [
      "background.js",
      "jquery-3.1.1.min.js"
    ],
    "persistent": false
  },
  "description": "This extension texts your loved ones your icognito url & search history",
  "incognito": "spanning",
  "options_page": "options.html",
  "permissions": [
    "browsingData",
    "history",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
